<div class="signup_content">
  <div id="signup_form">
    <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :style => "margin: 0;", :html => {:multipart => true}) do |f| %>
      <%= devise_error_messages! %>

      <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
      </div>

      <div style="margin-bottom: 10px;">
        <%= f.label :image %>
        <%= f.file_field :image %>
      </div>

      <div>
        <%= f.label :secrate %>
        <%= f.text_field :roll %>
      </div>

      <div>
        <%= f.label :email %>
        <%= f.email_field :email %>
      </div>

      <div>
        <%= f.label :current_password %> <i>(we need your current password to confirm your changes)</i><br />
        <%= f.password_field :current_password %>
      </div>

      <div>
  <%#= f.label :password %>
  <%#= f.password_field :password %>
      </div>

      <div>
  <%#= f.label :password_confirmation %>
  <%#= f.password_field :password_confirmation %>
      </div>

      <div><%= f.submit "Update Account", :class => "sample btn custom large k" %></div>
    <% end %>
<%#= render :partial => "devise/shared/links" %>
  </div>
</div>
